from pytermor import SequenceSGR, SeqIndex

start_color = 41
for idx, c in enumerate(range(start_color, start_color+(36*6), 36)):
    print(f\(aq{SequenceSGR.new_color_256(c)}███{SeqIndex.COLOR_OFF}\(aq, end=\(aq\(aq)

